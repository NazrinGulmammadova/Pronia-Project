@model ShippingItem
@{
    ViewData["Title"] = "Detail";
}

<div class="col-6">
<h1>Delete</h1>
<p>@Model.Description</p>
<img src="~/assets/images/shipping/icon/@Model.Image" height="50px"/>
</div>
<div class="col-12 mt-3">
    <form method="post">
<button class="btn btn-danger" asp-action="Delete">Delete</button>
</form>
<a class="btn btn-info" asp-action="Index">Go Back</a>
</div>




